{{#each errors}}
<div class="alert alert-danger">{{texto}}</div>
{{else}}

{{/each}}

<h2>{{customer.name}}</h2>
<div class="card">
    <div class="card-body">

    </div>
</div>
<hr>
<ul class="nav nav-tabs" id="myTab" role="tablist">
    {{#each dashboards}}
    <li class="nav-item" role="presentation">
        <button class="nav-link" id="{{id_dash}}-tab" data-bs-toggle="tab" data-bs-target="#{{id_dash}}" type="button"
            role="tab" aria-controls="{{id_dash}}" aria-selected="false">{{title}}</button>
    </li>
    {{else}}
    <h3>Nenhuma dashboard cadastrada</h3>
    {{/each}}
</ul>
<div class="tab-content" id="myTabContent">
    {{#each dashboards}}
    <div class="tab-pane fade" id="{{id_dash}}" role="tabpanel" aria-labelledby="{{id}}-tab">
        <iframe title="{{title}}" width="100%" height="541.25" src={{url_desktop}} frameborder="0"
            allowFullScreen="true"></iframe>
    </div>
    {{else}}

    {{/each}}
</div>